{
    "collection": "customerSingleView",
    "database": "mybank",
    "filters": [
        {
            "name": "consultantCustomers",
            "query": {
                "consultant.email": "%%user.data.email"
            },
            "apply_when": {
                "%%true": {
                    "%function": {
                        "name": "setConsultantFilter",
                        "arguments": [
                            "%%user.custom_data"
                        ]
                    }
                }
            }
        }
    ],
    "roles": [
        {
            "name": "default",
            "apply_when": {},
            "read": true,
            "insert": false,
            "delete": false,
            "search": true,
            "additional_fields": {}
        }
    ]
}
